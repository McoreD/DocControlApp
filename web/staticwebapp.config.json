{
  "routes": [
    {
      "route": "/api/auth/login",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/auth/register",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/auth/password/initial",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/*",
      "allowedRoles": [
        "authenticated"
      ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.{css,scss,js,png,gif,ico,jpg,svg,woff,woff2,ttf,eot}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css"
  }
}
